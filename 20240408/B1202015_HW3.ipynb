      "source": [
        "year = int(input(\"請輸入年分\"))\n",
        "if (year <= 1582):\n",
        "  if year % 4 == 0:\n",
        "    print(year,\"是閏年\")\n",
        "  else:\n",
        "    print(year,\"是平年\")\n",
        "else:\n",
        "  if year % 400 == 0 or year % 4 == 0 and year % 100 != 0:\n",
        "    print(year,\"是閏年\")\n",
        "  else:\n",
        "    print(year,\"是平年\")"
      ]
    }
